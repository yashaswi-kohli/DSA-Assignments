8. Calculate Depreciation of Value

#include <iostream>
#include <math.h>

using namespace std;
int main()
{
    int v, r, t;
    cout<<"Enter the Initial value: ";
    cin>>v;
    cout<<"Enter the rate of depreciation: ";
    cin>>r;
    cout<<"Enter the time(in years): ";
    cin>>t;
    float d = v * pow((1 - r / 100), t);
    cout<<"The deprecation value is: "<<d;
    
    return 0;
}

