23. Input a number and print all the factors of that number (use loops).

#include <iostream>
using namespace std;
int main()
{
    int n;
    cout<<"Enter a number: ";
    cin>>n;
    for(int i = 1; i <= n; i++)
    {
        if(n%i == 0)
        {
            cout<<i<<" ";
        }
    }
    return 0;
}

-------------------------------------------------------------------------------------------
24. Take integer inputs till the user enters 0 and print the sum of all numbers

#include <iostream>
using namespace std;
int main()
{
    int n, sum = 0;
    do
    {
        cout << "Enter number: ";
        cin >> n;
        sum = sum + n;
    } while(n != 0);
    cout << sum;
    return 0;
}

-------------------------------------------------------------------------------------------
25. Take integer inputs till the user enters 0 and print the largest number from all.

#include <iostream>
using namespace std;
int main()
{
    int n, max = 0;
    do
    {
        cout << "Enter number: ";
        cin >> n;
        if (n > max)
        {
            max = n;
        }
    } while (n != 0);
    cout << max;
    return 0;
}

