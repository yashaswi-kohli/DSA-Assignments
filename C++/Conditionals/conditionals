10. Calculate CGPA 

#include <iostream>
using namespace std;
int main()
{
   float n, t;
   float CGPA = 0;
   cout<<"Enter Number of Subjects: ";
   cin>>n;
   t = n;
   for(int i = n; i != 0; i--)
   {
      cout<<"Enter number: ";
      cin>>t;
      CGPA += t;
   }
   
   CGPA /= n;
   CGPA *=9.5;
   cout<<"The CGPA is: "<<CGPA;
   return 0;
}

-------------------------------------------------------------------------------------------
11. Compound Interest 

#include <iostream>
#include <cmath>
using namespace std;
int main()
{
   float r, t, p, ci;
   cout<<"Enter principal: ";
   cin>>p;
   cout<<"Enter rate: ";
   cin>>r;
   cout<<"Enter time in year's: ";
   cin>>t;
   ci = p * ( (pow( (1 + r / 100), t) - 1));
   cout<<"Compound Interest is: "<<ci;
   return 0;
}

