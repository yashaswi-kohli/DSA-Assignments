3. Calculate Average Of N Numbers


#include <iostream>
using namespace std;
int main()
{
   int n, t;
   float avg = 0;
   cout<<"Enter how many numbers of avg you want: ";
   cin>>n;
   t = n;
   for(int i = n; i != 0; i--)
   {
      cout<<"Enter number: ";
      cin>>t;
      avg += t;
   }
   avg /= n;
   cout<<"The avg of "<<n<<" number's is: "<<avg;
   return 0;
}

