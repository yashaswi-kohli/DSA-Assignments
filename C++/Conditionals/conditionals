7. Power

#include <iostream>
using namespace std;
int main()
{
    int a, b, i, n = 1;
    cout<<"Enter number and it's power: ";
    cin>>a>>b;
    for(i = 1; n <=b; n++)
    {
        i*=a;
    }
    
    cout<<a<<" power "<<b<<" is: "<<i<<".";
    
    return 0;
}

