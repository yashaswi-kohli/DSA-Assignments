using namespace std;
int main()
{
   int ld, sd, area;
   cout<<"Enter long diagonal and short diagonal: ";
   cin>>ld>>sd;
   
   area = (ld*sd*0.5);
  
   cout<<"Area is: "<<area;
   return 0;
}

