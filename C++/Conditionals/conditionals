4. Calculate Discount Of Product

#include <iostream>
using namespace std;
int main()
{
    int a, b, c, dis;
    cout<<"Enter the price of product & discount: ";
    cin>>a>>b;
    c = b*a/100;
    dis = a - c;
    cout<<"After "<<b<<"%"" discount the price of product is: "<<dis;
    
    return 0;
}

-------------------------------------------------------------------------------------------
5. Calculate Distance Between Two Points 

#include <iostream>
#include <math.h>
using namespace std;
int main()
{
    int x1, x2, y1, y2, d;
    cout<<"Enter the coordinate's of 1st point: ";
    cin>>x1>>y1;
    cout<<"Enter the coordinate's of 2nd point: ";
    cin>>x2>>y2;
    d = sqrt( ( (x2 - x1)*(x2 - x1) ) -  ( (y2 - y1)*(y2 - y1) ) );
    cout<<"The distance between two points is: "<<d;
    
    return 0;
}

