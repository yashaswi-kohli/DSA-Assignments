21. Fibonacci Series

-------------------------------------------

1st type of question


#include <iostream>
using namespace std;
int main()
{
    int a, b, n, i;
    cin >> a >> b >> n;
    for(i=a; i<=n; i=a+b)
    {
        cout<<i<<" ";  
        a = b;
        b = i;
    }
    
    return 0;
}

-------------------------------------------

2nd type of question


#include <iostream>
using namespace std;
int main()
{
    int a, b, n, i;
    cin >> a >> b >> n;
    for(int c = 2; c <= n; c++)
    {
        int d = b;
        b = b + a;
        a = d;
    }
    cout<<b;
    return 0;
}



