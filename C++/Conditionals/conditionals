20. HCF Of Two Numbers Program

#include <iostream>
using namespace std;
int main()
{
    int a, b;
    cout<<"Enter two number: ";
    cin>>a>>b;
    for(int i = (a>b)?b:a; i <=(a>b)?b:a; i--)
    {
        if( ((a>b)?a:b) % i == 0)
        {
            cout<<i<<" is the HCF of "<<a<<" and "<<b<<".";
            break;
        }
    }
    
    return 0;
}

-------------------------------------------------------------------------------------------
21. LCM Of Two Numbers

#include <iostream>
using namespace std;
int main()
{
    int a, b, c=1;
    cout<<"Enter two number: ";
    cin>>a>>b;
    for (int i = (a>b)?a:b; i <= (a*b); i *= c)
    {
        if(i%b==0)
        {
            cout<<i<<" is the LCM for "<<a<<" and "<<b<<".";
            break;
        }
        c++;
    }
    
    return 0;
}

