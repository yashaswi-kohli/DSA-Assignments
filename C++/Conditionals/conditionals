14. Sum Of N Numbers

#include <iostream>
using namespace std;
int main()
{
   int n, t, sum;
   cout<<"Enter how many numbers of sum you want: ";
   cin>>n;
   
   for(int i = n; i != 0; i--)
   {
      cout<<"Enter number: ";
      cin>>t;
      sum += t;
   }
   cout<<"The sum of "<<n<<" number's is: "<<sum;
   return 0;
}

-------------------------------------------------------------------------------------------
15. Armstrong Number 

#include <iostream>
using namespace std;
int main()
{
   int a, b = 0, c;
   cout<<"Enter a number: ";
   cin>>a;
   
   for(int i = a; i > 0; i /= 10)
   {
      
      c = (i%10);
      c = (c*c*c);
      b += c;
   }
   if(a == b)
   {
      cout<<a<<" is a Armstrong number.";
   }
   else
   {
      cout<<a<<" is not a Armstrong number.";
   }
   return 0;
}

-------------------------------------------------------------------------------------------
16. Find Ncr & Npr

#include <iostream>
using namespace std;
int main()
{
   int n, r, a = 1,d =1, f = 1;
   cout<<"Enter n & r: ";
   cin>>n>>r;
   for(int i = n; i > 0; i--)
   {
      a *= i;
   }
   for(int b = r; b > 0; b--)
   {
      d *= b;
   }
   for(int e = (n-r); e > 0; e--)
   {
      f *= e;
   }
   float P = a/f;
   float C = a/(d*f);
   cout<<"Permutation: "<<P<<endl;
   cout<<"Combination: "<<C;
   return 0;
}

-------------------------------------------------------------------------------------------
17. Reverse A String 

#include <iostream>
using namespace std;
int main()
{
   string a;
   cout<<"Enter a string: ";
   cin>>a;
   for(int i = a.size(); i >= 0; i--)
   {
      cout<<a[i]<<"";
   }
   return 0;
} 

-------------------------------------------------------------------------------------------
18. Find if a number is palindrome or not 

#include <iostream>
using namespace std;
int main()
{
   int a, b, c=0;
   cout<<"Enter a number: ";
   cin>>a;
   
   for(int i = a; i > 0; i /= 10)
   {
      
      b = (i%10);
      c = (c*10)+b;
   }
   if(a == c)
   {
      cout<<a<<" is a palindrome number.";
   }
   else
   {
      cout<<a<<" is not a palindrome number.";
   }
   return 0;
}

